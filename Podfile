# platform :ios, '9.0'

target 'wgs' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for wgs
  pod 'R.swift'

  # Pods for Networking
  pod 'Alamofire'

  # Data Persistance
   pod 'RealmSwift', '3.13.1'

  # Pods for UI
  pod 'SDWebImage'

  # Pods logging
  pod 'CocoaLumberjack/Swift', '~> 3.2.0'

  target 'wgsTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
